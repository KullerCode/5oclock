<div class="container">
    <% if (user) { %>
      <a class ="log-in-text-main-page" href="/logout">LOG OUT</a>
    <% } else { %>
      <a class ="log-in-text-main-page" href="/auth/google">LOG IN WITH GOOGLE</a>
    <% } %>
  <div class="center-container">
    <h1 class="title">Its 5 o'clock somewhere</h1>
  </div>
</div> 

<div class="icons-container">
  <img src="https://i.imgur.com/BknJzrE.png" height="50" width="70">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
